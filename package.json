    "url": "http://tran-engineering.ch"
  },
  "dependencies": {
    "@types/archiver": "^0.15.33",
    "@types/bluebird": "^3.0.35",
    "@types/lodash": "^4.14.37",
    "@types/node": "^6.0.45",
    "@types/rimraf": "0.0.28",
    "@types/xml2js": "0.0.28",
    "archiver": "^1.0.0",
    "awesome-typescript-loader": "^2.2.4",
    "bluebird": "^3.3.5",
    "clean-webpack-plugin": "^0.1.13",
    "dev-null": "^0.1.1",
    "extract-zip": "^1.5.0",
    "fs": "0.0.1-security",
    "fstream": "^1.0.8",

  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "remap-istanbul": "^0.6.3",
    "tslint": "^3.15.1"
  },
  "scripts": {
    "build": "webpack",
    "install": "webpack",
    "test": "istanbul cover _mocha --report lcovonly -- -R spec && remap-istanbul --exclude kexcel/webpack,kexcel/~ -i coverage/coverage.json -o coverage/coverage-remapped.lcov -t lcovonly && cat ./coverage/coverage-remapped.lcov | coveralls",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && remap-istanbul --exclude kexcel/webpack,kexcel/~ -i coverage/coverage.json -o coverage/report -t html",
    "localtest": "mocha"
  },
  "config": {
    "blanket": {
      "pattern": [
        "js"
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  }
}
